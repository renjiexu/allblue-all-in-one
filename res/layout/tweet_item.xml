<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent"
	android:paddingBottom="4dip"
	android:paddingLeft="12dip"
	>
	<ImageView android:id="@+id/user_icon"
		android:minHeight="40dip" android:minWidth="40dip"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:paddingRight="5dip"></ImageView>
	<TextView android:text="ItemText"
		android:layout_toRightOf="@+id/user_icon"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/ItemText">
	</TextView>
	<TextView android:text="ItemUser" android:layout_height="wrap_content"
		android:layout_toRightOf="@+id/user_icon"
		android:layout_below="@+id/ItemText" android:layout_width="wrap_content"
		android:id="@+id/ItemUser">
	</TextView>
	<TextView android:text="" android:layout_height="0px"
		android:layout_width="0px"
		android:id="@+id/ItemInReplyTo">
	</TextView>
	<TextView android:text="" android:layout_height="0px"
		android:layout_width="0px"
		android:id="@+id/ItemId">
	</TextView>
	<TextView android:text="" android:layout_height="0px"
		android:layout_width="0px"
		android:id="@+id/ItemScreenName">
	</TextView>
</RelativeLayout>